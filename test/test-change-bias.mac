# minimal command set test
# verbose
/run/verbose 1
/event/verbose 0
/tracking/verbose 0

# set default cut
/run/setCut 3.0 cm

# bias commands
/WLGD/bias/setNeutronBias 10.0
/WLGD/bias/setMuonBias 20.0

# run init
/run/initialize

# lab depth [km.w.e.]
/WLGD/generator/depth 5.89

# default input file name
/WLGD/generator/setMUSUNFile ./musun_gs_100M.dat

# start
/run/beamOn 4

