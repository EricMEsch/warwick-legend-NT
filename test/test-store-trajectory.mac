# minimal command set test
# verbose
/run/verbose 2
/tracking/verbose 0

# Enable trajectory storage
/tracking/storeTrajectory 1

# set default cut
/run/setCut 3.0 cm

# run init
/run/initialize

# lab depth [km.w.e.]
/WLGD/generator/depth 5.89

# default input file name
/WLGD/generator/setMUSUNFile ./musun_gs_100M.dat

# start
/run/beamOn 4

